<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0/EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.unity_backend.Dao.User.Register.RegisterMapper">
    <sql id="baseColumn">
        Username
    </sql>

    <select id="getAllGameRecord" resultType="com.example.unity_backend.Entity.GameRecord">
        select * from GameRecords where Username= #{username}
    </select>


</mapper>
